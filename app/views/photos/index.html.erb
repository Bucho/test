<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
		<title>OLOLO</title>
	</head>
	<body>
		<h1>Listing Photos</h1>
		
		<table>
    	<% for photo in @photos %>
		<tr>
			<td>
				<%= link_to(image_tag("#{photo.thumbnail}",
										:size => '75x56',
										:border => 1,
										:id => 'thumbnail'),
								url_for(:action => 'show', :id => photo)
								)
				%>
			</td>
			<td>
				<%=h photo.filename %>
				<br>
				<%= link_to 'delete me', { :action => 'destroy', :id => photo },
			   							   :confirm => 'Are you sure?' %>
				<td><%= link_to 'right_working_destroy', photo_path(photo), :method => :delete, :confirm => 'Are you sure?' %></td>
			</td>
		</tr>
		<% end %>
	    </table>
		
		</br>
		
		<%= link_to 'New photo', :action => 'new' %>
		</body>
</html>