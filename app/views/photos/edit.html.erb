<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
		<title>Edit Photo</title>
	</head>
	<body>
		<h1>Editing photo</h1>
		<% form_for(@photo) do |f| %>
			<%= f.error_messages %>
			<%= render :partial => "form", :locals => {:f => f} %>
			<%= submit_tag "Edit" %>
		<% end %>
		
		<%= link_to 'Show', photo_path(@photo) %> |
		<%= link_to 'Back', photos_path %>
	</body>
</html>
